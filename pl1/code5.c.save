#include<stdio.h>
int main(){
    int p[3][5];
    int n[24];
    int i, j, count, index;
    for(i=0; i<4; i++){
    for(j=0; j<6; j++){
        scanf("%d", &p[i][j]);
        printf("Number in array index[%d][%d]= %d \n",i,j, p[i][j]);
        }
    }
    for(j=0; j<4; j++){
    for(i=0; i<6; i++){
        if(60 > p[i][j]){
        n[index] = p[i][j];
        index++;
        count++;
        }
        }
        }
    for(i=0; i<count; i++){
        printf("New array index[%d]=%d \n", i, n[i]);
        }
    }

