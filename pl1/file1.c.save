#include<stdio.h>
int main(){
    int num1, num2, sum;
    FILE *fp_in, *fp_out;
    char line[80];
    fp_in = fopen("in.txt", "r");
    fp_out = fopen("out.txt", "w");
    fgets(line, 80, fp_in);
    printf("Line %s \n", line);
    sscanf(line, "%d %d", &num1, &num2);
    sum = num1+num2; 
    printf("%d ")
    fprintf(fp_out, "%d\n", sum);
    
    }
